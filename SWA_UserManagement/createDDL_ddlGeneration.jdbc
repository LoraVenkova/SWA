CREATE TABLE COMPANY (COMPANYID INTEGER NOT NULL, ADDRESS VARCHAR(255), DEPARTMENT VARCHAR(255), NAME VARCHAR(255), PRIMARY KEY (COMPANYID))
CREATE TABLE LICENSE (LICENSEID INTEGER NOT NULL, EXPIRATIONDATE VARCHAR(255), IPNUMBER VARCHAR(255), LICENSECOUNT VARCHAR(255), LICENSEKEY VARCHAR(255), SERVICECONTRACT_SERVICECONTRACTID INTEGER, PRIMARY KEY (LICENSEID))
CREATE TABLE SERVICECONTRACT (SERVICECONTRACTID INTEGER NOT NULL, ENDDATE DATE, STARTDATE DATE, COMPANY_COMPANYID INTEGER, PRIMARY KEY (SERVICECONTRACTID))
CREATE TABLE users (USERID INTEGER AUTO_INCREMENT NOT NULL, CURRENT_CONNECTIONS BIGINT, EMAILADDRESS VARCHAR(255), ISSYSTEMADMIN TINYINT, NAME VARCHAR(255), PASSWORD TINYINT, TOTAL_CONNECTIONS BIGINT, USER VARCHAR(255), USERNAME VARCHAR(255), USERSCOL VARCHAR(255), COMPANY_COMPANYID INTEGER, PRIMARY KEY (USERID))
ALTER TABLE LICENSE ADD CONSTRAINT FK_LICENSE_SERVICECONTRACT_SERVICECONTRACTID FOREIGN KEY (SERVICECONTRACT_SERVICECONTRACTID) REFERENCES SERVICECONTRACT (SERVICECONTRACTID)
ALTER TABLE SERVICECONTRACT ADD CONSTRAINT FK_SERVICECONTRACT_COMPANY_COMPANYID FOREIGN KEY (COMPANY_COMPANYID) REFERENCES COMPANY (COMPANYID)
ALTER TABLE users ADD CONSTRAINT FK_users_COMPANY_COMPANYID FOREIGN KEY (COMPANY_COMPANYID) REFERENCES COMPANY (COMPANYID)
CREATE TABLE SEQUENCE (SEQ_NAME VARCHAR(50) NOT NULL, SEQ_COUNT DECIMAL(38), PRIMARY KEY (SEQ_NAME))
